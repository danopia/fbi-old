{{# project }}
<h2>Recent commits to <a href="/projects/{{ slug }}">{{ title }}</a>{{# repo }}&nbsp;/ <a href="/projects/{{ slug }}/repos/{{ id }}">{{ short_name }}</a>{{/ repo }}</h2>

<hr />
{{/ project }}

<table class="fixedwidth" width="100%">
  <tr>
    <th width="20%">Repository</th>
    <th width="20%">Age</th>
    <th width="60%">Message</th>
  </tr>
{{# commits2 }}
  <tr>
    <td>{{#repo}}<a href="/projects/ooc/commits/repos/{{id}}">{{short_name}}</a>{{/repo}}</td>
    <td>{{ committed_date }}</td>
    <td>{{ short_message }} [<a href="/projects/ooc/commits/authors/{{author}}">{{author}}</a>]</td>
  </tr>
{{/ commits2 }}
</table>
