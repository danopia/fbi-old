<table>
  <tr>
    <th>Repo</th>
    <th>Commiter</th>
    <th>Time</th>
    <th>Message</th>
    <th>Hash</th>
  </tr>
{{# commits }}
  <tr>
    <td>{{#repo}}<a href="/projects/ooc/repos/{{id}}">{{short}}</a>{{/repo}}</td>
    <td>{{#author}}<a href="/projects/ooc/authors/{{name}}">{{name}}</a>{{/author}}</td>
    <td>{{ committed_date }}</td>
    <td>{{ short_message }}</td>
    <td>{{ short_hash }}</td>
  </tr>
{{/ commits }}
</table>
